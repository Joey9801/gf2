{
    includes : {
        example_defs/4bit-counter.def : counter,
        example_defs/full_adder.def : adder
    },
    components : {
        clock : {
            type : siggen,
            config : {
                data : 01,
                period : 5
            }
        },
        counter : {
            type : includes.counter,
            inputs : {
                clock : clock
            }
        },
        adder : {
            type : includes.adder,
            inputs : {
                a : counter.count[0],
                b : counter.count[1],
                c_in : counter.count[2]
            }
        }
    }
}
